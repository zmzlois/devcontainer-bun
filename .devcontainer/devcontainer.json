{
    "name": "Bun",
    "features": {
        "ghcr.io/devcontainers/features/github-cli:1": {},
        "ghcr.io/devcontainers/features/docker-in-docker:2": {}
    },
    "build": {
        "dockerfile": "Dockerfile",
        // Update 'VARIANT' to pick a Bun version.
        "args": {
            "VARIANT": "latest"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "dbaeumer.vscode-eslint"
            ]
        }
    }
}